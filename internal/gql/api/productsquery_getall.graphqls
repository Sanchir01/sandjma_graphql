extend type ProductQuery {
     getAllProduct(
         sort:ArticleBlockFindSortEnum! = SORT_RANK_ASC
     ):GetAllProductResult!  @goField(forceResolver: true)
}

type GetAllProductsOk {
    products:[Product!]!
}
enum ArticleBlockFindSortEnum {
    CREATED_AT_ASC
    CREATED_AT_DESC
    SORT_RANK_ASC
    SORT_RANK_DESC
}
union GetAllProductResult = | GetAllProductsOk | InternalErrorProblem